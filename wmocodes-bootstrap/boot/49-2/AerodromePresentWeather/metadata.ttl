@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix reg:     <http://purl.org/linked-data/registry#> .
@prefix ldp:     <http://www.w3.org/ns/ldp#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .

<_AerodromePresentWeather>
    a reg:RegisterItem ;
    reg:definition [ reg:entity <AerodromePresentWeather> ] ;
    reg:itemClass reg:Register ;
    reg:notation "AerodromePresentWeather" ;
    reg:status reg:statusStable ;
    .
<AerodromePresentWeather>
    a reg:Register , ldp:Container , skos:Collection ;
    ldp:membershipPredicate skos:member ;
    rdfs:label "Aerodrome present weather" ;
    dct:description "This register is intended specifically for aeronautical purposes; it lists those weather phenomena that may be observed at an aerodrome as defined in WMO No. 49 Vol II - Meteorological Services for Air Navigation clause [_CITATION REQUIRED_]. A super-set of cloud-type categories are defined in WMO No. 306 Vol I code-table 4678 'Significant weather'."@en ;
    reg:owner <http://codes.wmo.int/system/organization/wmo> ;
    reg:manager <http://codes.wmo.int/system/organization/www-dm> ;
    .