<_SigConvectiveCloudType>
    a reg:RegisterItem ;
    reg:definition [ reg:entity <SigConvectiveCloudType> ] ;
    reg:itemClass reg:Register ;
    reg:notation "SigConvectiveCloudType" ;
    reg:status reg:statusStable ;
