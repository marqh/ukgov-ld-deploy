@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix reg:     <http://purl.org/linked-data/registry#> .
@prefix ldp:     <http://www.w3.org/ns/ldp#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .

<_observable-property>
    a reg:RegisterItem ;
    reg:definition [ reg:entity <observable-property> ] ;
    reg:itemClass reg:Register ;
    reg:notation "observable-property" ;
    reg:status reg:statusExperimental ;
    .
<observable-property>
    a reg:Register , ldp:Container , skos:Collection ;
    ldp:membershipPredicate skos:member ;
    rdfs:label "Observable property resources" ;
    dct:description "This register is intended specifically for aeronautical purposes; its members are composite observable properties that define the set of physical properties evaluated as a result of regulated procedures such as aerodrome observation and forecast reports (e.g. METAR, SPECI and TAF). The set of physical properties associated with each composite observable property is derived from analysis of the technical regulation ICAO Annex 3 (WMO No. 49 Vol 2) 'Meteorological Services for International Air Navigation'."@en ;
    reg:owner <http://codes.wmo.int/system/organization/wmo> ;
    reg:manager <http://codes.wmo.int/system/organization/www-dm> ;
    .