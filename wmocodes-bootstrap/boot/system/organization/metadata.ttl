@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix skos:    <http://www.w3.org/2004/02/skos/core#> .
@prefix reg:     <http://purl.org/linked-data/registry#> .
@prefix ldp:     <http://www.w3.org/ns/ldp#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .

<organization>
    a reg:Register , ldp:Container ;
    rdfs:label "Organization"@en ;
    dct:description "This system register provides authoritative URI for organizations involved in the maintenance of this Registry in lieu of an external URI being available."@en ;
    reg:containedItemClass foaf:Organization ;
    .